class Solution {
public:
    int subtractProductAndSum(int n) {
         int ld =0;
        int sum=0;
        int prod=1;
        while(n!=0){
            ld=n%10;
            prod=prod*ld;
            sum=sum+ld;
            n=n/10;
        }
        int ans=prod-sum;
        return ans;
    }
};
